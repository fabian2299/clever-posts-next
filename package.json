{
  "name": "prueba",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "build:test": "npm run db:reset && export NODE_ENV=\"test\" && next build",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cypress:open": "env-cmd -f .env.test cypress open",
    "cypress:start": "start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
    "cypress:build": "npm run build:test && npm run cypress:start",
    "cypress:run": "start-server-and-test 'npm run start:test' 3000 'env-cmd -f .env.test cypress run'"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "cookies-next": "^2.1.1",
    "i18next": "^22.0.4",
    "next": "13.0.0",
    "next-i18next": "^12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.6.0",
    "sweetalert2": "^11.6.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/node": "18.11.7",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.8",
    "cypress": "^10.11.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "msw": "^0.39.2",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.6.4"
  }
}
